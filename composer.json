{
    "name": "pointybeard/symphony-api-framework",
    "description": "JSON renderer and event driven controller interface to help rapidly prototype and build RESTful APIs in Symphony CMS",
    "homepage": "http://alannahkearney.com",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Alannah Kearney",
            "email": "hi@alannahkearney.com"
        }
    ],
    "support": {
      "issues": "https://github.com/pointybeard/api_framework/issues",
      "wiki": "https://github.com/pointybeard/api_framework/wiki"
    },
    "require": {
        "php": ">=7.3",
        "symfony/http-foundation": "~4.2.0",
        "justinrainbow/json-schema": "~5.2.0",
        "pointybeard/symphony-section-builder": "^0.2.0",
        "pointybeard/symphony-classmapper": "^2.0.0",
        "pointybeard/symphony-pdo": "^0.1.7.0",
        "pointybeard/helpers": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Symphony\\Console\\Commands\\Api_Framework\\": "commands",
            "pointybeard\\Symphony\\Extensions\\": "src",
            "pointybeard\\Symphony\\Extensions\\Api_Framework\\Controllers\\": "../../workspace/controllers"
        },
        "files": [
            "src/Includes/Functions.php",
            "extension.driver.php"
        ]
    }
}
