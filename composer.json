{
    "name": "pointybeard/symphony-api-framework",
    "version": "1.1.0",
    "description": "JSON renderer and event driven controller interface for SymphonyCMS which is designed to help rapidly prototype and build RESTful APIs.",
    "homepage": "http://alannahkearney.com",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Alannah Kearney",
            "email": "hi@alannahkearney.com"
        }
    ],
    "support": {
      "issues": "https://github.com/pointybeard/api_framework/issues",
      "wiki": "https://github.com/pointybeard/api_framework/wiki"
    },
    "autoload": {
        "psr-4": {
            "Symphony\\Console\\Commands\\Api_Framework\\": "commands",
            "Symphony\\ApiFramework\\ApiFramework\\": "src/ApiFramework",
            "Symphony\\ApiFramework\\Controllers\\": "../../workspace/controllers"
        },
        "files": [
            "src/Includes/Functions.php",
            "extension.driver.php"
        ]
    },
    "require": {
        "php": ">=7.3",
        "symfony/http-foundation": "~4.2",
        "justinrainbow/json-schema": "~5.2",
        "pointybeard/symphony-section-builder": "~0.1",
        "pointybeard/symphony-classmapper": "dev-feature-2.0.0-php72",
        "pointybeard/symphony-pdo": "~0.1",
        "pointybeard/helpers-functions-arrays": "~1.0",
        "pointybeard/helpers-cli-message": "~1.0",
        "pointybeard/helpers-cli-input": "~1.0"
    }
}
